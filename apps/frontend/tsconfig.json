{
	"extends": ["@hooper/tsconfig/next.json"],
	"compilerOptions": {
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"**/*.js",
		"**/*.cjs",
		"**/*.mjs"
	],
	"exclude": ["node_modules", "playwright-report", "test-results"]
}
